		<field xsi:type="DataSetFieldField">
			<dataPath>ДлительностьРазговора</dataPath>
			<field>ДлительностьРазговора</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДатаНачалаЗвонка</dataPath>
			<field>ДатаНачалаЗвонка</field>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
