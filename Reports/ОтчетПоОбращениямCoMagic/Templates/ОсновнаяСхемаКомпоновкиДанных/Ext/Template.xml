	Взаимодействия.Контакт КАК Контакт,
	Взаимодействия.ДатаВзаимодействия КАК ДатаВзаимодействия
ПОМЕСТИТЬ ВТ_Взаимодействия
ИЗ
	РегистрСведений.Взаимодействия КАК Взаимодействия
ГДЕ
	Взаимодействия.Взаимодействие В
			(ВЫБРАТЬ
				ВТ_ДанныеCallTracking.Взаимодействие КАК Взаимодействие
			ИЗ
				ВТ_ДанныеCallTracking КАК ВТ_ДанныеCallTracking)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_ДанныеCallTracking.ПериодОбращения КАК ПериодОбращения,
	ВТ_ДанныеCallTracking.ID КАК ID,
	ВТ_ДанныеCallTracking.Сайт КАК Сайт,
	ВТ_ДанныеCallTracking.НомерТелефона КАК НомерТелефона,
	ВТ_ДанныеCallTracking.РекламнаяКампания КАК РекламнаяКампания,
	ВТ_ДанныеCallTracking.ID_Звонка КАК ID_Звонка,
	ВТ_Взаимодействия.ДатаВзаимодействия КАК ДатаВзаимодействия,
	ВТ_Взаимодействия.АктивностьВзаимодействия КАК АктивностьВзаимодействия,
	ВТ_Взаимодействия.Проект КАК Проект,
	ВТ_Взаимодействия.Контакт КАК Клиент,
	ЕСТЬNULL(ВТ_ПараметрыЗапроса.Стадия, ЗНАЧЕНИЕ(Перечисление.СтадииЗапроса.ПустаяСсылка)) КАК Стадия,
	ЕСТЬNULL(ВТ_ПараметрыЗапроса.Статус, ЗНАЧЕНИЕ(Перечисление.СтатусыЗапроса.ПустаяСсылка)) КАК Статус,
	ВТ_ДанныеCallTracking.Запрос КАК Запрос,
	ВТ_ДанныеCallTracking.Взаимодействие КАК Взаимодействие,
	ВТ_ДанныеCallTracking.СостояниеЗвонка КАК СостояниеЗвонка,
	ВТ_ДанныеCallTracking.СтатусЗвонка КАК СтатусЗвонка,
	ВТ_ДанныеCallTracking.ДатаНачалаЗвонка КАК ДатаНачалаЗвонка,
	ВТ_ДанныеCallTracking.ВызывающийНомер КАК ВызывающийНомер,
	ВТ_ДанныеCallTracking.ВызываемыйНомер КАК ВызываемыйНомер,
	ВТ_ДанныеCallTracking.КонтактЗвонка КАК КонтактЗвонка,
	ВТ_ДанныеCallTracking.UTM_Source КАК UTM_Source,
	ВТ_ДанныеCallTracking.UTM_Campaign КАК UTM_Campaign,
	ВТ_ДанныеCallTracking.UTM_Medium КАК UTM_Medium,
	ВТ_ДанныеCallTracking.UTM_Term КАК UTM_Term
{ВЫБРАТЬ
	ПериодОбращения,
	ID,
	Сайт.*,
	НомерТелефона,
	РекламнаяКампания.*,
	ID_Звонка,
	Стадия,
	Статус,
	Запрос.*,
	Взаимодействие.*,
	СостояниеЗвонка.*,
	СтатусЗвонка.*,
	UTM_Source.*,
	UTM_Campaign.*,
	UTM_Medium.*,
	UTM_Term.*}
ИЗ
	ВТ_ДанныеCallTracking КАК ВТ_ДанныеCallTracking
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ПараметрыЗапроса КАК ВТ_ПараметрыЗапроса
		ПО ВТ_ДанныеCallTracking.Запрос = ВТ_ПараметрыЗапроса.Запрос
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Взаимодействия КАК ВТ_Взаимодействия
		ПО ВТ_ДанныеCallTracking.Взаимодействие = ВТ_Взаимодействия.Взаимодействие
{ГДЕ
	ВТ_ДанныеCallTracking.ПериодОбращения,
	ВТ_ДанныеCallTracking.ID,
	ВТ_ДанныеCallTracking.Сайт.*,
	ВТ_ДанныеCallTracking.НомерТелефона,
	ВТ_ДанныеCallTracking.РекламнаяКампания.*,
	ВТ_ДанныеCallTracking.ID_Звонка,
	ВТ_ДанныеCallTracking.Контакт.*,
	(ЕСТЬNULL(ВТ_ПараметрыЗапроса.Стадия, ЗНАЧЕНИЕ(Перечисление.СтадииЗапроса.ПустаяСсылка))) КАК Стадия,
	(ЕСТЬNULL(ВТ_ПараметрыЗапроса.Статус, ЗНАЧЕНИЕ(Перечисление.СтатусыЗапроса.ПустаяСсылка))) КАК Статус,
	ВТ_ДанныеCallTracking.Запрос.*,
	ВТ_ДанныеCallTracking.Взаимодействие.*,
	ВТ_ДанныеCallTracking.СостояниеЗвонка.*,
	ВТ_ДанныеCallTracking.СтатусЗвонка.*,
	ВТ_ДанныеCallTracking.UTM_Source.*,
	ВТ_ДанныеCallTracking.UTM_Campaign.*,
	ВТ_ДанныеCallTracking.UTM_Medium.*,
	ВТ_ДанныеCallTracking.UTM_Term.*}</query>
	</dataSet>
	<parameter>
		<name>ПериодКонец</name>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Период конец</v8:content>
			</v8:item>
		</title>
		<valueType>
			<v8:Type>xs:dateTime</v8:Type>
			<v8:DateQualifiers>
				<v8:DateFractions>DateTime</v8:DateFractions>
			</v8:DateQualifiers>
		</valueType>
		<value xsi:type="xs:dateTime">0001-01-01T00:00:00</value>
		<useRestriction>true</useRestriction>
		<expression>&amp;СтандартныйПериод.ДатаОкончания</expression>
	</parameter>
	<parameter>
		<name>ПериодНачало</name>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Период начало</v8:content>
			</v8:item>
		</title>
		<valueType>
			<v8:Type>xs:dateTime</v8:Type>
			<v8:DateQualifiers>
				<v8:DateFractions>DateTime</v8:DateFractions>
			</v8:DateQualifiers>
		</valueType>
		<value xsi:type="xs:dateTime">0001-01-01T00:00:00</value>
		<useRestriction>true</useRestriction>
		<expression>&amp;СтандартныйПериод.ДатаНачала</expression>
	</parameter>
	<parameter>
		<name>СтандартныйПериод</name>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Стандартный период</v8:content>
			</v8:item>
